{
  "name": "gmssc",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:local": "npm run gs-api && ng serve -- --proxyConfig=proxy.conf.json",
    "gs-api": "npm run gs-api:extract && npm run gs-api:swagger-gen",
    "gs-api:extract": "node ./src/tools/swagger/src/swagger.json 0.0.1-SNAPSHOT",
    "gs-api:swagger-gen": "./node_modules/.bin/ng-swagger-gen -i ./src/tools/swagger/src/swagger.json -o src/gs-api/src"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "bootstrap": "^4.6.1",
    "highcharts": "^9.3.2",
    "highcharts-angular": "^3.0.0",
    "jquery": "^3.6.0",
    "ngx-pagination": "^5.1.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.13",
    "@angular/cli": "~12.2.0",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "mkdirp": "^1.0.4",
    "ng-packagr": "^5.3.0",
    "ng-swagger-gen": "^1.7.0-b",
    "typescript": "~4.3.5"
  }
}
